{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": ["eslint:recommended", "prettier", "plugin:prettier/recommended"],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error",
        "no-unused-vars": ["error", { "ignoreRestSiblings": true }]
    }
}
